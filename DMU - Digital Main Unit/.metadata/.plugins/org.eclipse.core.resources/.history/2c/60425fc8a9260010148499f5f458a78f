typedef struct
{
	uint16 position_x;
	uint16 position_y;
	uint16 width;
	uint16 height;
}DigitalCluster_RectangleBox_t;
typedef struct
{
	uint16 position_x;
	uint16 position_y;
	char* text;
}DigitalCluster_TextInBox_t;
typedef struct
{
	uint16 position_x1;
	uint16 position_y1;
	uint16 position_x2;
	uint16 position_y2;
}DigitalCluster_DiagonalLine_t;
typedef struct
{
	uint16 position_x;
	uint16 position_y;
	uint16 length;
}DigitalCluster_HorizontalLine_t;
typedef struct
{
	DigitalCluster_DiagonalLine_t SpeedQuarter;
}DigitalCluster_SpeedDiagLines_t;
typedef struct
{
	DigitalCluster_DiagonalLine_t RpmQuarter;
}DigitalCluster_RpmDiagLines_t;
typedef struct
{
	DigitalCluster_TextInBox_t SL_KMH;
	DigitalCluster_TextInBox_t SL_RPM;
}DigitalCluster_CenterScreen_t;
typedef struct
{
	DigitalCluster_RectangleBox_t TimeBox;
	DigitalCluster_TextInBox_t T_HourText;
	DigitalCluster_TextInBox_t T_DotDotText;
	DigitalCluster_TextInBox_t T_MinuteText;
}DigitalCluster_Time_t;
typedef struct
{
	DigitalCluster_RectangleBox_t TemperatureBox;
	DigitalCluster_TextInBox_t T_TempText;
}DigitalCluster_Temp_t;
typedef struct
{
	DigitalCluster_RectangleBox_t GearSelectedBox;
	DigitalCluster_TextInBox_t G_GselText;
}DigitalCluster_G_t;
typedef struct
{
	DigitalCluster_RectangleBox_t CheckControlMsgBox;
	DigitalCluster_TextInBox_t CCM_Text;
}DigitalCluster_CCM_t;
typedef struct
{
	DigitalCluster_RectangleBox_t DASBox;
	DigitalCluster_TextInBox_t DAS_Text;
}DigitalCluster_DAS_t;
typedef struct
{
	DigitalCluster_RectangleBox_t DashboardLightsBox;
	DigitalCluster_TextInBox_t DL_HB;
	DigitalCluster_TextInBox_t DL_PS;
}DigitalCluster_DL_t;
typedef struct
{
	DigitalCluster_RectangleBox_t DashboardLightsBox2;
	DigitalCluster_TextInBox_t DL2_FLF;
	DigitalCluster_TextInBox_t DL2_FLR;
	DigitalCluster_TextInBox_t DL2_RLS;
	DigitalCluster_TextInBox_t DL2_C;
	DigitalCluster_TextInBox_t DL2_RT;
	DigitalCluster_TextInBox_t DL2_RTN;
	DigitalCluster_TextInBox_t DL2_AUTO;
	DigitalCluster_TextInBox_t DL2_FV;
	DigitalCluster_TextInBox_t DL2_FVN;
}DigitalCluster_DL_t2;
typedef struct
{
	DigitalCluster_RectangleBox_t TSBox;
}DigitalCluster_TS_t;
typedef struct
{
	uint16 position_x;
	uint16 position_y;
	uint16 width;
	uint16 height;
}DigitalCluster_RevCam_t;
typedef struct
{
	DigitalCluster_RectangleBox_t KMTOTBox;
	DigitalCluster_TextInBox_t KMTOT_Text;
	DigitalCluster_TextInBox_t KMTOTStat_Text;
}DigitalCluster_KmTot_t;
typedef struct
{
	DigitalCluster_RectangleBox_t KMDCYBox;
	DigitalCluster_TextInBox_t KMDCY_Text;
}DigitalCluster_KmDcy_t;
typedef struct
{
	DigitalCluster_Time_t Time;
	DigitalCluster_Temp_t Temperature;
	DigitalCluster_G_t Gear;
	DigitalCluster_CCM_t CheckControlMessage;
	DigitalCluster_DAS_t DriverAssistanceMessage;
	DigitalCluster_DL_t DashboardLights;
	DigitalCluster_DL_t2 DashboardLights2;
	DigitalCluster_TS_t TurnSignalsBox;
	DigitalCluster_CenterScreen_t CenterScreenInformation;
	DigitalCluster_SpeedDiagLines_t SpeedBar;
	DigitalCluster_RpmDiagLines_t RpmBar;
	DigitalCluster_KmTot_t KmTot;
	DigitalCluster_KmDcy_t KmDcy;
	uint8 DisplayType;
	uint16 DisplayColor;
	uint16 DisplayRPMColor;
	uint16 DisplayVehSpeedColor;
}DigitalCluster_DisplayMode_t;
extern void DigitalCluster_MainFunction(void);

#endif /* DIGITALCLUSTER_H_ */
