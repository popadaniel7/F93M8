#include "Ifx_Types.h"
#include "Can.h"

#define COMMASTER_NO_RX_MSG 10u
#define COMMASTER_NO_TX_MSG 10u

typedef struct
{
        Can_TxMsg_t transmitMessage;
        uint8 transmitFlag;
        uint32 cycleTime;
}ComMaster_TransmitType_t;

typedef struct
{
        Can_RxMsg_t receiveMessage;
        uint8 receivedValidFlag;
}ComMaster_ReceiveType_t;

