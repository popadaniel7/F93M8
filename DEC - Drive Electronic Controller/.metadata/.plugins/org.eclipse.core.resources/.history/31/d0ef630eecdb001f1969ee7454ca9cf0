#include "Ifx_Types.h"

#define ENCCAL_CALIBRATION_SIZE 35U
#define ENCCAL_CODING_SIZE      21U

typedef struct
{
        uint8 model[8];
        uint8 vin[17];
        uint8 chassisNumber[3];
        uint8 upholstery[24];
        uint8 paint[24];
        uint8 swVersion[4][3];
        uint8 hwVersion[4][3];
        uint8 calibrationVersion[4][3];
        uint8 codingVersion[4][3];
        uint8 optionList[20][64];
        uint8 buildDate[8];
        uint8 modulesList[4][64];
}EncCal_VOData_t;

