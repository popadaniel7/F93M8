                                .storeInTxFifoQueue = FALSE,
                                .readFromRxFifo0 = FALSE,
                                .readFromRxFifo1 = FALSE
                        },
                },
                .transmitFlag = 1u,
                .cycleTime = 1u,
        },
        /* BusSystemDateAndTime */
        {
                .transmitMessage =
                {
                        .txMsg =
                        {
                                .bufferNumber = 9u,
                                .messageId = 0x201u,
                                .remoteTransmitRequest = 0u,
