<ifxmlcfg version="2.1.12">
  <checksum>20ae48b0a4f83218a300188277a0929f3c0c26e25e75c0a204424340023e39e9</checksum>
  <version label="V0.1.2"/>

  <verticalcontainer label="CCU7" desc="Capture Compare Unit 7"> 
    <header file="ccu7_defines.h">

      <checkbox label="Enable CCU7" define="CCU7_EN" svd="0" font="Source Sans Pro,15,0,0,100,1,0,0,0,0" color="255;128;64">
        <action event="checked"   cmd="setDef" data="CCU7_MODULE.enabled=1"/>
        <action event="unchecked" cmd="setDef" data="CCU7_MODULE.enabled=0"/>
      </checkbox>

      <horizontalcontainer define="CCU7_MODULE">

        <verticalcontainer label="left">
          <groupcontainer label="Timer, Clock" font="Source Sans Pro,12,0,0,100,0,0,0,0,0" color="171;217;228">
            <tabcontainer>
              <!-- T12 -->
              <include TIMER_IDX="T12" TCTR2_REGISTER_IDX="" TCTR2_FIELD_IDX="2">
                template_ccu7_t12.xml</include> 

              <!-- T13 -->
              <include TIMER_IDX="T13" CMPVAL_IDX="C73" TCTR2_REGISTER_IDX="" TCTR2_FIELD_IDX="3" T1xTEC_0="SR[0]" T1xTEC_1="T12_CM_70" T1xTEC_2="T12_CM_71" T1xTEC_3="T12_CM_72" T1xTEC_4="Any Compare Event (T12_CM_7x)" T1xTEC_5="T12_PM" T1xTEC_6="T12_ZM and CDIR = 0" T1xTEC_7="Any Hall State Change">
                template_ccu7_t13.xml</include> 

              <!-- T14 -->
              <include TIMER_IDX="T14" CMPVAL_IDX="C74" TCTR2_REGISTER_IDX="4" TCTR2_FIELD_IDX="x" T1xTEC_0="SR[1]" T1xTEC_1="T12_CM_70" T1xTEC_2="T12_CM_70B" T1xTEC_3="T15_CM_75" T1xTEC_4="T16_CM_76" T1xTEC_5="T12_PM" T1xTEC_6="T12_ZM and CDIR = 0" T1xTEC_7="T13_PM">
                template_ccu7_t141516.xml</include> 

              <!-- T15 -->
              <include TIMER_IDX="T15" CMPVAL_IDX="C75" TCTR2_REGISTER_IDX="5" TCTR2_FIELD_IDX="x" T1xTEC_0="SR[2]" T1xTEC_1="T12_CM_71" T1xTEC_2="T12_CM_71B" T1xTEC_3="T14_CM_74" T1xTEC_4="T16_CM_76" T1xTEC_5="T12_PM" T1xTEC_6="T12_ZM and CDIR = 0" T1xTEC_7="T14_PM">
                template_ccu7_t141516.xml</include> 

              <!-- T16 -->
              <include TIMER_IDX="T16" CMPVAL_IDX="C76" TCTR2_REGISTER_IDX="6" TCTR2_FIELD_IDX="x" T1xTEC_0="SR[3]" T1xTEC_1="T12_CM_72" T1xTEC_2="T12_CM_72B" T1xTEC_3="T14_CM_74" T1xTEC_4="T15_CM_75" T1xTEC_5="T12_PM" T1xTEC_6="T12_ZM and CDIR = 0" T1xTEC_7="T15_PM">
                template_ccu7_t141516.xml</include> 

              <!-- CCU7 Clock Settings -->
              <treecontainer label="CCU7 Clock" headerlabels="Item;Value;" columnwidths="250;250;1">
                <!-- fPWM -->
                <math label="fCCU7" define="CCU7_FCCU7" svd="0" formula="SCU_fCCU7_MHz" unit="MHz"/>

                <!-- fFDIV0 -->
                <branch label="fFDIV0">
                  <!-- FDIV0 Mode Selection is fixed to 0B (8by8) -->
                  <math label="FDIV0 Setting" define="CCU7.T_CLK_CTRL.FDIV0_SEL" formula="0"/>
                  <spinbox label="P" define="CCU7_T_FDIV0_P" svd="0" header="temp_defines.h" default="0" range="0;255"/>
                  <spinbox label="Q" define="CCU7_T_FDIV0_Q" svd="0" header="temp_defines.h" default="0" range="0;255"/>
                  <math label="Fractional Divider fDIV0" visible="false" define="CCU7.T_FDIV0.PQ" formula="CCU7_T_FDIV0_P*256 + CCU7_T_FDIV0_Q"/>
                  <math label="CCU7 Clock 0" unit="MHz" define="CCU7_FDIV0_CLK" svd="0" formula="CCU7_T_FDIV0_Q==0?CCU7_FCCU7:CCU7_FCCU7*CCU7_T_FDIV0_P/CCU7_T_FDIV0_Q">
                    <action event="changed" cond="CCU7_FDIV0_CLK&lt;60" cmd="setDef" data="CCU7_FDIV0_CLK_ERROR=0"/>
                    <action event="changed" cond="CCU7_FDIV0_CLK==60" cmd="setDef" data="CCU7_FDIV0_CLK_ERROR=0"/>
                    <action event="changed" cond="CCU7_FDIV0_CLK&gt;60" cmd="setDef" data="CCU7_FDIV0_CLK_ERROR=1"/>
                  </math>
                </branch>

                <!-- fFDIV1 -->
                <branch label="fFDIV1">
                  <!-- FDIV1 Mode Selection is fixed to 0B (8by8) -->
                  <math label="FDIV1 Setting" define="CCU7.T_CLK_CTRL.FDIV1_SEL" formula="0"/>
                  <spinbox label="P" define="CCU7_T_FDIV1_P" svd="0" header="temp_defines.h" default="0" range="0;255"/>
                  <spinbox label="Q" define="CCU7_T_FDIV1_Q" svd="0" header="temp_defines.h" default="0" range="0;255"/>
                  <math label="Fractional Divider fDIV1" visible="false" define="CCU7.T_FDIV1.PQ" formula="CCU7_T_FDIV1_P*256 + CCU7_T_FDIV1_Q"/>
                  <math label="CCU7 Clock 1" unit="MHz" define="CCU7_FDIV1_CLK" svd="0" formula="CCU7_T_FDIV1_Q==0?CCU7_FCCU7:CCU7_FCCU7*CCU7_T_FDIV1_P/CCU7_T_FDIV1_Q">
                    <action event="changed" cond="CCU7_FDIV1_CLK&lt;60" cmd="setDef" data="CCU7_FDIV1_CLK_ERROR=0"/>
                    <action event="changed" cond="CCU7_FDIV1_CLK==60" cmd="setDef" data="CCU7_FDIV1_CLK_ERROR=0"/>
                    <action event="changed" cond="CCU7_FDIV1_CLK&gt;60" cmd="setDef" data="CCU7_FDIV1_CLK_ERROR=1"/>
                  </math>
                </branch>

                <combo label="Add. 1/256 prescaler for T14, T15, and T16" define="CCU7.TCTR1.T1xPRE">
                  <item label="Add. 1/256 prescaler disabled" override="value1" lockon="CCU7_T14_ADDPRESC=0;CCU7_T15_ADDPRESC=0;CCU7_T16_ADDPRESC=0"/>
                  <item label="Add. 1/256 prescaler enabled" override="value2" lockon="CCU7_T14_ADDPRESC=1;CCU7_T15_ADDPRESC=1;CCU7_T16_ADDPRESC=1"/>
                </combo>
              </treecontainer>
            </tabcontainer>
            
            <treecontainer label="Clock value update" headerlabels="Item;Value;" columnwidths="250;250;1" visible="false">
              <text label="Needed because py can't connect to maths directly"/>
              <edit label="T12 CLK" define="CCU7_T12_CLK_EDIT" svd="0"/>
              <edit label="T13 CLK" define="CCU7_T13_CLK_EDIT" svd="0"/>
              <edit label="T14 CLK" define="CCU7_T14_CLK_EDIT" svd="0"/>
              <edit label="T15 CLK" define="CCU7_T15_CLK_EDIT" svd="0"/>
              <edit label="T16 CLK" define="CCU7_T16_CLK_EDIT" svd="0"/>
            </treecontainer>    
              
            <verticalcontainer visible="false" label="Clock error variables">
              <!-- combos needed to raise messagebox in case of clock error -->
              <combo label="CCU7_FDIV0_CLK_ERROR" define="CCU7_FDIV0_CLK_ERROR" header="temp_defines.h" svd="0">
                <item value="0" label="no error">
                </item>
                <item value="1" label="clock error">
                  <action event="selected" cmd="runScript" data="MessageBox(Clock can not be more than 60MHz! P and Q were set to 0. Please make sure Q is always larger than P.)"/>
                  <action event="selected" cmd="setDef" data="CCU7_T_FDIV0_P=0;CCU7_T_FDIV0_Q=0"/>
                </item>
              </combo>
              <combo label="CCU7_FDIV1_CLK_ERROR" define="CCU7_FDIV1_CLK_ERROR" header="temp_defines.h" svd="0">
                <item value="0" label="no error">
                </item>
                <item value="1" label="clock error">
                  <action event="selected" cmd="runScript" data="MessageBox(Clock can not be more than 60MHz! P and Q were set to 0. Please make sure Q is always larger than P.)"/>
                  <action event="selected" cmd="setDef" data="CCU7_T_FDIV1_P=0;CCU7_T_FDIV1_Q=0"/>
                </item>
              </combo>
            </verticalcontainer>

          </groupcontainer>

          <!-- Deadtime Configuration -->
          <groupcontainer label="Deadtime Configuration" font="Source Sans Pro,12,0,0,100,0,0,0,0,0" color="206;217;164"  size="-1;400">
              <treecontainer label="Deadtime Configuration" headerlabels="Item;Value;" columnwidths="250;250;1">
                <combo label="Clock Select" define="CCU7.T_CLK_CTRL.DT_CLK_SEL">
                  <item label="fCCU7: No prescaler selected" override="value1"/>
                  <item label="fFDIV0: Clock from Fractional Divider 0" override="value2"/>
                  <item label="fFDIV1: Clock from Fractional Divider 1" override="value3"/>
                </combo>
                <math label="Deadtime Clock" unit="MHz" define="CCU7_DT_CLK" svd="0" formula="CCU7.T_CLK_CTRL.DT_CLK_SEL==0?CCU7_FCCU7:(CCU7.T_CLK_CTRL.DT_CLK_SEL==1?CCU7_FDIV0_CLK:CCU7_FDIV1_CLK)"/>

                <radio label="Deadtime option" define="CCU7_DTSEL_OPTION" svd="0" header="temp_defines.h" default="1">
                  <radiobutton label="General Deadtime for all Signals" value="0" uielement="branch">
                    <checkbox label="Enable Deadtime for all Channels" define="CCU7_T12DTC_DTE_ALL" svd="0" header="temp_defines.h" uielement="branch" lockon="CCU7.T12DTC.DTE0=CCU7_T12DTC_DTE_ALL;CCU7.T12DTC.DTE1=CCU7_T12DTC_DTE_ALL;CCU7.T12DTC.DTE2=CCU7_T12DTC_DTE_ALL">
                      <doublespinbox label="Deadtime value [us]" define="CCU7_DEADTIME_ALL" svd="0" header="temp_defines.h">
                        <action event="changed" cmd="setDef" data="CCU7_CH0_DEADTIME_CC7=CCU7_DEADTIME_ALL;CCU7_CH0_DEADTIME_COUT7=CCU7_DEADTIME_ALL"/>
                        <action event="changed" cmd="setDef" data="CCU7_CH1_DEADTIME_CC7=CCU7_DEADTIME_ALL;CCU7_CH1_DEADTIME_COUT7=CCU7_DEADTIME_ALL"/>
                        <action event="changed" cmd="setDef" data="CCU7_CH2_DEADTIME_CC7=CCU7_DEADTIME_ALL;CCU7_CH2_DEADTIME_COUT7=CCU7_DEADTIME_ALL"/>
                      </doublespinbox>
                      <math label="Deadtime value in ticks" define="CCU7_DEADTIME_ALL_TICKS" svd="0" header="temp_defines.h" formula="CCU7_DT_CLK*CCU7_DEADTIME_ALL"/>              
                    </checkbox>   
                  </radiobutton>
                  <radiobutton label="Individual Deadtime" value="1" uielement="branch">
                    <checkbox label="Enable Deadtime for Channel 0" define="CCU7.T12DTC.DTE0" uielement="branch">
                      <doublespinbox label="Deadtime for CC70 [us]" define="CCU7_CH0_DEADTIME_CC7" svd="0" header="temp_defines.h"/>
                      <math label="Deadtime for CC70x in ticks" define="CCU7.T12DT0_VAL.DTM_RISE" formula="CCU7_DT_CLK*CCU7_CH0_DEADTIME_CC7"/>
                      <doublespinbox label="Deadtime for COUT70 [us]" define="CCU7_CH0_DEADTIME_COUT7" svd="0" header="temp_defines.h"/>
                      <math label="Deadtime for COUT70x in ticks" define="CCU7.T12DT0_VAL.DTM_FALL" formula="CCU7_DT_CLK*CCU7_CH0_DEADTIME_COUT7"/>                
                    </checkbox>            
                    <checkbox label="Enable Deadtime for Channel 1" define="CCU7.T12DTC.DTE1" uielement="branch">
                      <doublespinbox label="Deadtime for CC71 [us]" define="CCU7_CH1_DEADTIME_CC7" svd="0" header="temp_defines.h"/>
                      <math label="Deadtime for CC71x in ticks" define="CCU7.T12DT1_VAL.DTM_RISE" formula="CCU7_DT_CLK*CCU7_CH1_DEADTIME_CC7"/>
                      <doublespinbox label="Deadtime for COUT71 [us]" define="CCU7_CH1_DEADTIME_COUT7" svd="0" header="temp_defines.h"/>
                      <math label="Deadtime for COUT71x in ticks" define="CCU7.T12DT1_VAL.DTM_FALL" formula="CCU7_DT_CLK*CCU7_CH1_DEADTIME_COUT7"/>                
                    </checkbox>            
                    <checkbox label="Enable Deadtime for Channel 2" define="CCU7.T12DTC.DTE2" uielement="branch">
                      <doublespinbox label="Deadtime for CC72 [us]" define="CCU7_CH2_DEADTIME_CC7" svd="0" header="temp_defines.h"/>
                      <math label="Deadtime for CC72x in ticks" define="CCU7.T12DT2_VAL.DTM_RISE" formula="CCU7_DT_CLK*CCU7_CH2_DEADTIME_CC7"/>
                      <doublespinbox label="Deadtime for COUT72 [us]" define="CCU7_CH2_DEADTIME_COUT7" svd="0" header="temp_defines.h"/>
                      <math label="Deadtime for COUT72x in ticks" define="CCU7.T12DT2_VAL.DTM_FALL" formula="CCU7_DT_CLK*CCU7_CH2_DEADTIME_COUT7"/>                
                    </checkbox>                    
                  </radiobutton>
                </radio>

              </treecontainer>
          </groupcontainer>

        </verticalcontainer>

        <verticalcontainer label="mid" size="-1;1000">
          <groupcontainer label="Modulation Configuration" font="Source Sans Pro,12,0,0,100,0,0,0,0,0" color="214;126;177">
            <verticalcontainer>
              <horizontalcontainer stretch="2">
                <text size="82;-1" label="   Signal"/>
                <text size="220;-1" label="Modulated by Deadtime Channel/T13..."/>
                <text size="95;-1" label="Passive State"/>
                <text size="90;-1" label="Passive Level"/>
                <text size="90;-1" label="Invert Signal"/>
              </horizontalcontainer>
              
              <groupcontainer><verticalcontainer>
                <horizontalcontainer label="dtinsel0" stretch="2">
                  <text size="75;-1" label="DT Ch0 Input"/>
                  <combo size="120;-1" define="CCU7.T12DTINSEL.DTINSEL0">
                    <item override="value1" label="CC70A State Bit"/>
                    <item override="value2" label="C70A/B State Bit"/>
                    <item override="value3" label="T14 Run Bit"/>
                    <item override="value4" label="T14 State Bit"/>
                  </combo>
                </horizontalcontainer>                <horizontalcontainer stretch="2">
                  <text     size="75;-1" label="CC70"/>
                  <checkbox size="120;-1" label="DT Ch0" define="CCU7.MODCTR.T12MODEN_CC70"/>
                  <checkbox size="90;-1" label="T13" define="CCU7.MODCTR.T13MODEN_CC70"/>
                  <combo    size="95;-1" label="Passive State Select" define="CCU7.CMPSTAT.CC70PS" default="label=before Cmp">
                    <item label="before Cmp" override="value1"/>
                    <item label="after Cmp" override="value2"/>
                  </combo>
                  <combo    size="90;-1" label="Passive State Level" define="CCU7.PSLR.PSL_CC70" default="label=Low">
                    <item label="Low" override="value1"/>
                    <item label="High" override="value2"/>
                  </combo>
                  <text     size="22;-1" label="n/a"/>
                </horizontalcontainer>
                <horizontalcontainer stretch="2">
                  <text     size="75;-1" label="COUT70"/>
                  <checkbox size="120;-1" label="DT Ch0" define="CCU7.MODCTR.T12MODEN_COUT70"/>
                  <checkbox size="90;-1" label="T13" define="CCU7.MODCTR.T13MODEN_COUT70"/>
                  <combo    size="95;-1" label="Passive State Select" define="CCU7.CMPSTAT.COUT70PS" default="label=after Cmp">
                    <item label="before Cmp" override="value1"/>
                    <item label="after Cmp" override="value2"/>
                  </combo>
                  <combo    size="90;-1" label="Passive State Level" define="CCU7.PSLR.PSL_COUT70" default="label=Low">
                    <item label="Low" override="value1"/>
                    <item label="High" override="value2"/>
                  </combo>
                  <text     size="22;-1" label="n/a"/>
                </horizontalcontainer>
              </verticalcontainer></groupcontainer>
              
              <groupcontainer><verticalcontainer>
                <horizontalcontainer label="dtinsel1" stretch="2">
                  <text size="75;-1" label="DT Ch1 Input"/>
                  <combo size="120;-1" define="CCU7.T12DTINSEL.DTINSEL1">
                    <item override="value1" label="CC71A State Bit"/>
                    <item override="value2" label="C71A/B State Bit"/>
                    <item override="value3" label="T15 Run Bit"/>
                    <item override="value4" label="T15 State Bit"/>
                  </combo>
                </horizontalcontainer>
                <horizontalcontainer stretch="2">
                  <text     size="75;-1" label="CC71"/>
                  <checkbox size="120;-1" label="DT Ch1" define="CCU7.MODCTR.T12MODEN_CC71"/>
                  <checkbox size="90;-1" label="T13" define="CCU7.MODCTR.T13MODEN_CC71"/>
                  <combo    size="95;-1" label="Passive State Select" define="CCU7.CMPSTAT.CC71PS" default="label=before Cmp">
                    <item label="before Cmp" override="value1"/>
                    <item label="after Cmp" override="value2"/>
                  </combo>
                  <combo    size="90;-1" label="Passive State Level" define="CCU7.PSLR.PSL_CC71" default="label=Low">
                    <item label="Low" override="value1"/>
                    <item label="High" override="value2"/>
                  </combo>
                  <text     size="22;-1" label="n/a"/>
                </horizontalcontainer>
                <horizontalcontainer stretch="2">
                  <text     size="75;-1" label="COUT71"/>
                  <checkbox size="120;-1" label="DT Ch1" define="CCU7.MODCTR.T12MODEN_COUT71"/>
                  <checkbox size="90;-1" label="T13" define="CCU7.MODCTR.T13MODEN_COUT71"/>
                  <combo    size="95;-1" label="Passive State Select" define="CCU7.CMPSTAT.COUT71PS" default="label=after Cmp">
                    <item label="before Cmp" override="value1"/>
                    <item label="after Cmp" override="value2"/>
                  </combo>
                  <combo    size="90;-1" label="Passive State Level" define="CCU7.PSLR.PSL_COUT71" default="label=Low">
                    <item label="Low" override="value1"/>
                    <item label="High" override="value2"/>
                  </combo> 
                  <text     size="22;-1" label="n/a"/>
                </horizontalcontainer>

              </verticalcontainer></groupcontainer>
              
              <groupcontainer><verticalcontainer>
                <horizontalcontainer label="dtinsel2" stretch="2">
                  <text size="75;-1" label="DT Ch2 Input"/>
                  <combo size="120;-1" define="CCU7.T12DTINSEL.DTINSEL2">
                    <item override="value1" label="CC72A State Bit"/>
                    <item override="value2" label="C72A/B State Bit"/>
                    <item override="value3" label="T16 Run Bit"/>
                    <item override="value4" label="T16 State Bit"/>
                  </combo>
                </horizontalcontainer>
                <horizontalcontainer stretch="2">
                  <text     size="75;-1" label="CC72"/>
                  <checkbox size="120;-1" label="DT Ch2" define="CCU7.MODCTR.T12MODEN_CC72"/>
                  <checkbox size="90;-1" label="T13" define="CCU7.MODCTR.T13MODEN_CC72"/>
                  <combo    size="95;-1" label="Passive State Select" define="CCU7.CMPSTAT.CC72PS" default="label=before Cmp">
                    <item label="before Cmp" override="value1"/>
                    <item label="after Cmp" override="value2"/>
                  </combo>
                  <combo    size="90;-1" label="Passive State Level" define="CCU7.PSLR.PSL_CC72" default="label=Low">
                    <item label="Low" override="value1"/>
                    <item label="High" override="value2"/>
                  </combo>
                  <text     size="22;-1" label="n/a"/>
                </horizontalcontainer>
                <horizontalcontainer stretch="2">
                  <text     size="75;-1" label="COUT72"/>
                  <checkbox size="120;-1" label="DT Ch2" define="CCU7.MODCTR.T12MODEN_COUT72"/>
                  <checkbox size="90;-1" label="T13" define="CCU7.MODCTR.T13MODEN_COUT72"/>
                  <combo    size="95;-1" label="Passive State Select" define="CCU7.CMPSTAT.COUT72PS" default="label=after Cmp">
                    <item label="before Cmp" override="value1"/>
                    <item label="after Cmp" override="value2"/>
                  </combo>
                  <combo    size="90;-1" label="Passive State Level" define="CCU7.PSLR.PSL_COUT72" default="label=Low">
                    <item label="Low" override="value1"/>
                    <item label="High" override="value2"/>
                  </combo>
                  <text     size="22;-1" label="n/a"/>
                </horizontalcontainer>
              </verticalcontainer></groupcontainer>
              
              <groupcontainer><verticalcontainer>
                <horizontalcontainer stretch="2">
                  <text     size="75;-1" label="COUT73"/>
                  <text     size="120;-1" label="n/a"/>
                  <checkbox size="90;-1" label="T13" define="CCU7.MODCTR.ECT13O"/>
                  <combo    size="95;-1" label="Passive State Select" define="CCU7.CMPSTAT.COUT73PS" default="label=after Cmp">
                    <item label="before Cmp" override="value1"/>
                    <item label="after Cmp" override="value2"/>
