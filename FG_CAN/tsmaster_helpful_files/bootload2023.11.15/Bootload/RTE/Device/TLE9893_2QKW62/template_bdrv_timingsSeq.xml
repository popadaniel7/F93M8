<!-- HB_IDX Half-bridge number -->
<!-- CYCLE Charge or Discharge -->
<!-- ONOFF On or Off -->
<!-- PHASE Phase number -->
<!-- DEFAULT Default value -->

<ifxmlcfg version="2.6.2">
  <checksum>94c965dd1bb429a18a5cc03d2bd11d97c99cf7959bb934a83508a50d77745a13</checksum>
  <version label="V0.1.5"/>
  <svd file="tle989x.svd"/>
  <usesPython/>
  <horizontalcontainer define="BDRV.HB$HB_IDX$_T$PHASE$$CYCLE$SEQ_CONT" size="350;-1">
    <header file="bdrv_defines.h">
      <spinbox label="Phase $PHASE$ - Time [50ns - 3.2us]" define="BDRV.HS$HB_IDX$SEQ$ONOFF$TC_HS$HB_IDX$_T$PHASE$$ONOFF$" default="$DEFAULT$" range="50;3200" step="50" svd="0" size="70;-1" header="temp_defines.h">
        <action event="changed" cmd="setDef" data="BDRV_UPDATE.run=1"/>
      </spinbox>
      <text label="Reg:" size="35;-1"/>
      <math label="Register Value" define="BDRV.HS$HB_IDX$SEQ$ONOFF$TC_HS$HB_IDX$_T$PHASE$$ONOFF$_MATH" svd="0" format="%1X" formula="round((BDRV.HS$HB_IDX$SEQ$ONOFF$TC_HS$HB_IDX$_T$PHASE$$ONOFF$/50)-1)" header="temp_defines.h"/>
      <spinbox label="Phase $PHASE$ - Time [50ns - 3.2us]" define="BDRV.LS$HB_IDX$SEQ$ONOFF$TC_LS$HB_IDX$_T$PHASE$$ONOFF$" default="$DEFAULT$" range="50;3200" step="50" svd="0" size="70;-1" header="temp_defines.h">
        <action event="changed" cmd="setDef" data="BDRV_UPDATE.run=1"/>
      </spinbox>
      <text label="Reg:" size="35;-1"/>
      <math label="Register Value" define="BDRV.LS$HB_IDX$SEQ$ONOFF$TC_LS$HB_IDX$_T$PHASE$$ONOFF$_MATH" svd="0" format="%1X" formula="round((BDRV.LS$HB_IDX$SEQ$ONOFF$TC_LS$HB_IDX$_T$PHASE$$ONOFF$/50)-1)" header="temp_defines.h"/>
    </header>
  </horizontalcontainer>
</ifxmlcfg>
