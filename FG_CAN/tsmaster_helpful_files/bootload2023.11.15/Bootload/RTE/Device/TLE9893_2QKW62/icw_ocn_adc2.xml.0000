              <var  define="ADC2.USE_FILTx"              svd="0" default="0"           header=""/>
              <var  define="ADC2.USE_CMPx"               svd="0" default="0"           header=""/>
              <var  define="ADC2.CMP_SRCx"               svd="0" default="0"           header=""/>
              <var  define="ADC2.INBOX_WIDTH"            svd="0" default="160"         header=""/>
              <var  define="ADC2.BOX_XPOS"               svd="0" default="200"         header=""/>
              <math define="ADC2.INBOX_WIDTH_HALFE"      svd="0" visible="false"       header="" formula="ADC2.INBOX_WIDTH/2"/>
              <math define="ADC2.INBOX_WIDTH_HALFE_p5"   svd="0" visible="false"       header="" formula="ADC2.INBOX_WIDTH_HALFE+5"/>
              <math define="ADC2.INBOX_WIDTH_HALFE_m5"   svd="0" visible="false"       header="" formula="ADC2.INBOX_WIDTH_HALFE-5"/>
              <math define="ADC2.INBOX_XPOS"             svd="0" visible="false"       header="" formula="ADC2.BOX_XPOS-100+(350-ADC2.INBOX_WIDTH)/2"/>
              <math define="ADC2.AD_CAL_LINK_XPOS"       svd="0" visible="false"       header="" formula="ADC2.USE_CALIBx==1?ADC2.BOX_XPOS:1000"/>
              <math define="ADC2.AD_RES_LINK_XPOS"       svd="0" visible="false"       header="" formula="ADC2.USE_CALIBx==0?ADC2.BOX_XPOS:1000"/>
              <math define="ADC2.CAL_RES_LINK_XPOS"      svd="0" visible="false"       header="" formula="ADC2.USE_FILTx==0?ADC2.BOX_XPOS:1000"/>
              <math define="ADC2.CAL_RES_FILT_LINK_XPOS" svd="0" visible="false"       header="" formula="ADC2.USE_FILTx==1?ADC2.BOX_XPOS-100:1000"/>
              <math define="ADC2.DEAD_CMP_LINK_XPOS"     svd="0" visible="false"       header="" formula="((ADC2.USE_FILTx.^1)+ADC2.USE_CMPx+ADC2.CMP_SRCx)==3?ADC2.BOX_XPOS:1000"/>
              <math define="ADC2.RES_CMP_LINK_XPOS"      svd="0" visible="false"       header="" formula="((ADC2.USE_FILTx.^1)+ADC2.USE_CMPx+(ADC2.CMP_SRCx.^1))==3?ADC2.BOX_XPOS:1000"/>
              <math define="ADC2.RES_FILT_CMP_LINK_XPOS" svd="0" visible="false"       header="" formula="(ADC2.USE_FILTx+ADC2.USE_CMPx+ADC2.CMP_SRCx)==3?ADC2.BOX_XPOS-100:1000"/>
              <math define="ADC2.FILT_RES_CMP_LINK_XPOS" svd="0" visible="false"       header="" formula="(ADC2.USE_FILTx+ADC2.USE_CMPx+(ADC2.CMP_SRCx.^1))==3?ADC2.BOX_XPOS-100:1000"/>
              <math define="ADC2.CAL_XPOS"               svd="0" visible="false"       header="" formula="ADC2.USE_CALIBx==1?ADC2.BOX_XPOS:1000"/>
