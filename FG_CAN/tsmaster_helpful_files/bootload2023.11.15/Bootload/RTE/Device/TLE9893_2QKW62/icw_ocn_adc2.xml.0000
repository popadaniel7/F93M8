              <edit define="ADC2.CMP_MODE_TXT"           svd="0" default="---"         header="" visible="false"/>
              <edit define="ADC2.CMP_UTH_TXT"            svd="0" default="---"         header="" visible="false"/>
              <edit define="ADC2.CMP_LTH_TXT"            svd="0" default="---"         header="" visible="false"/>
              <edit define="ADC2.CH_NAME"                svd="0" default="VS (3..35V)" header="" visible="false"/>
              <edit define="ADC2.CHCLS_CTRY"             svd="0" default="n/a"         header="" visible="false"/>
              <edit define="ADC2.RES_NO"                 svd="0" default="0"           header="" visible="false"/>
              <var  define="ADC2.FILT_NO"                svd="0" default="0"           header=""/>
              <var  define="ADC2.CMP_NO"                 svd="0" default="0"           header=""/>
