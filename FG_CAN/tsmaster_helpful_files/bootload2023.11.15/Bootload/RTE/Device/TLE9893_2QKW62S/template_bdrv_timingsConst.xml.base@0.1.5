<!-- HB_IDX: Half-bridge number -->
<!-- ONOFF: On or Off -->

<ifxmlcfg version="2.6.2">
  <checksum>ee47ea39e5a5d8dd1f22c02e27255b6b475cb0525641648b5d72d489c5c78dc6</checksum>
  <version label="V0.1.5"/>
  <svd file="tle989x.svd"/>
  <usesPython/>
  <horizontalcontainer size="350;-1">
    <header file="bdrv_defines.h">
      <spinbox label="Time [50ns - 12.8us]" define="BDRV.HS$HB_IDX$AFTC_HS$HB_IDX$_TAF$ONOFF$" default="1050" range="50;12800" step="50" svd="0" size="70;-1" header="temp_defines.h">
        <action event="changed" cmd="setDef" data="BDRV_UPDATE.run=1"/>
      </spinbox>
      <text label="Reg:" size="35;-1"/>
      <math label="Register Value" define="BDRV.HS$HB_IDX$AFTC.HS$HB_IDX$_TAF$ONOFF$" format="%1X" formula="round((BDRV.HS$HB_IDX$AFTC_HS$HB_IDX$_TAF$ONOFF$/50)-1)"/>
      <spinbox label="Time [50ns - 12.8us]" define="BDRV.LS$HB_IDX$AFTC_LS$HB_IDX$_TAF$ONOFF$" default="1050" range="50;12800" step="50" svd="0" size="70;-1" header="temp_defines.h">
        <action event="changed" cmd="setDef" data="BDRV_UPDATE.run=1"/>
      </spinbox>
      <text label="Reg:" size="35;-1"/>
      <math label="Register Value" define="BDRV.LS$HB_IDX$AFTC.LS$HB_IDX$_TAF$ONOFF$" format="%1X" formula="round((BDRV.LS$HB_IDX$AFTC_LS$HB_IDX$_TAF$ONOFF$/50)-1)"/>
    </header>
  </horizontalcontainer>
</ifxmlcfg>

