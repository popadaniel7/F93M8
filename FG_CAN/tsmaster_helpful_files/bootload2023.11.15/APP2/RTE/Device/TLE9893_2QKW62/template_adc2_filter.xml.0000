<!-- Filter Template -->
<!-- Parameters: -->
<!--   FILTIDX: filter index, range 0..7 -->
<ifxmlcfg version="2.1.12">
  <checksum>5a6475c950e78828c3f643575ede8792d18a6e59940a5766cc27f909114153fd</checksum>
  <version label="V0.0.1"/>
  <svd file="tle989x.svd"/>
  <usesPython/>
  <treecontainer headerlabels="Item;Value;" label="Filter $FILTIDX$" desc="Filter Channel $FILTIDX$" columnwidths="300;300">
    <header file="adc2_defines.h">
      <!-- Variables -->
      <edit label="Used by Channel" readonly="1" define="ADC2.FILT$FILTIDX$_USED_TXT" svd="0" default="available" header="temp_defines.h"/>
      <!-- Configuration -->
      <combo label="Filter Coefficient" define="ADC2.FILTCFG.COEF_A$FILTIDX$"/>
    </header>
  </treecontainer>
</ifxmlcfg>
