      <var define="ADC1.CONV_TICKS_CHx" svd="0" default="0" header="temp_defines.h"/>
      <var define="ADC1.CAL_TICKS_CHx" svd="0" default="4" header="temp_defines.h"/>
      <var define="ADC1.FILT_TICKS_CHx" svd="0" default="1" header="temp_defines.h"/>
      <var define="ADC1.CMP_TICKS_CHx" svd="0" default="1" header="temp_defines.h"/>
      <var define="ADC1.CHREPx" svd="0" default="0" header="temp_defines.h"/>
      <edit visible="false" define="ADC1.GREEN" svd="0" default="CCE9AD" header="temp_defines.h"/>
      <edit visible="false" define="ADC1.YELLOW" svd="0" default="FFC90E" header="temp_defines.h"/>
      <math visible="false" define="ADC1.TIME_KERNEL" svd="0" format="%.2f" formula="(ADC1.CONV_TICKS_CHx+ADC1.CONV_TICKS_CHx*ADC1.CHREPx)/ADC1.CLK" header="temp_defines.h"/>
      <math visible="false" define="ADC1.TICKS_IN2RES_SINGLE" svd="0" format="%.2f" formula="ADC1.CONV_TICKS_CHx+ADC1.USE_CALIBx*ADC1.CAL_TICKS_CHx" header="temp_defines.h"/>
      <math visible="false" define="ADC1.TICKS_IN2RES_REP" svd="0" format="%.2f" formula="ADC1.TICKS_IN2RES_SINGLE+ADC1.TICKS_IN2RES_SINGLE*ADC1.CHREPx" header="temp_defines.h"/>
      <math visible="false" define="ADC1.TIME_IN2RES" svd="0" format="%.2f" formula="ADC1.TICKS_IN2RES_REP/ADC1.CLK" header="temp_defines.h"/>
      <math visible="false" define="ADC1.TIME_IN2FILT" svd="0" format="%.2f" formula="(ADC1.TICKS_IN2RES_REP+ADC1.FILT_TICKS_CHx)/ADC1.CLK" header="temp_defines.h"/>
      <math visible="false" define="ADC1.TIME_IN2CMP" svd="0" format="%.2f" formula="ADC1.CMP_SRCx==0?(ADC1.TICKS_IN2RES_REP+ADC1.CMP_TICKS_CHx)/ADC1.CLK:(ADC1.TICKS_IN2RES_REP+ADC1.FILT_TICKS_CHx+ADC1.CMP_TICKS_CHx)/ADC1.CLK" header="temp_defines.h"/>
