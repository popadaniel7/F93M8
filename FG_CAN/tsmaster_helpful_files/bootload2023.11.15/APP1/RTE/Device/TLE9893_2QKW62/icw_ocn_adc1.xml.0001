                        <math label="ADC1 Clock" define="ADC1.CLK" svd="0" unit="MHz" formula="SCU_fADC1_MHz/ADC1.CLKCON_CLKDIVp1" header="temp_defines.h"/>
                        <math visible="false" define="ADC1.CLKCON.CLKDIV" formula="ADC1.CLKCON_CLKDIVp1-1">
                          <action event="changed" cmd="setDef" data="ADC1.TIM_UPDATE_GFX.run=1"/>
                        </math>
                        <spinbox label="Clock Divider [1..16]" define="ADC1.CLKCON_CLKDIVp1" svd="0" range="1;16" default="2" header="temp_defines.h"/>
                      </branch>
                      <checkbox label="Enable VAREF" define="ARVG.VAREF_CTRL.EN" default="1" uielement="branch" expand="1">
                        <checkbox label="VAREF Overcurrent" define="ARVG.VAREF_OC_INT_EN" svd="0" uielement="branch" header="isr_defines.h">
                          <checkbox label="Enable Interrupt" define="ARVG.VAREF_IEN.OC_IEN" header="isr_defines.h"/>
                          <combo label="Node Selection" define="SCU.INP0.INP_ARVG" header="isr_defines.h">
                            <item label="WARN_INP0_NVIC_IRQn" override="value1" lockon="CPU.NVIC_ISER.IRQEN0=1"/>
                            <item label="WARN_INP1_NVIC_IRQn" override="value2" lockon="CPU.NVIC_ISER.IRQEN1=1"/>
                          </combo>
                          <edit label="Callback" define="ARVG.VAREF_OC_CALLBACK" svd="0" default="place_your_function_callback_here" header="isr_defines.h"/>
